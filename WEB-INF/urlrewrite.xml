<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<!--

    Configuration file for UrlRewriteFilter
    http://www.tuckey.org/urlrewrite/

-->
<urlrewrite>

    <rule>
        <from>klassifikasjoner/(.*/?)js/(.*)</from>
        <to>/js/$2</to>
    </rule>

    <rule>
        <from>klassifikasjoner/(.*/?)static/(.*)</from>
        <to>/static/$2</to>
    </rule>

    <rule>
        <from>klassifikasjoner/(.*/?)images/(.*)</from>
        <to>/images/$2</to>
    </rule>

    <rule>
        <from>klassifikasjoner/(.*/?)css/(.*)</from>
        <to>/css/$2</to>
    </rule>

    <rule>
        <from>klassifikasjoner/(.*)</from>
        <to>/index.html</to>
    </rule>


</urlrewrite>

